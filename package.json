{
  "name": "proverbial-app",
  "version": "0.1.0",
  "description": "your proverbial app",
  "scripts": {
    "lint": "./node_modules/.bin/eslint './src/**/*'",
    "lint-staged": "lint-staged",
    "start": "http-server ./dist",
    "start:dev": "webpack-dev-server",
    "postinstall": "webpack",
    "format": "prettier --write 'src/*.{js,jsx,ts,tsx,yml,css,sass,json,md}'",
    "tsc": "tsc"
  },
  "author": "Tiago Fernandes <vf.tiago@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/vftiago/proverbial-app.git",
  "private": false,
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "@material-ui/core": "^4.1.0",
    "@material-ui/icons": "^4.1.0",
    "axios": "^0.21.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.13",
    "babel-preset-react-optimize": "^1.0.1",
    "core-js": "^3.1.3",
    "dotenv-webpack": "^1.7.0",
    "emotion": "^10.0.9",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "seedrandom": "^3.0.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.0.2",
    "webpack": "^4.33.0"
  },
  "devDependencies": {
    "@types/dotenv-webpack": "^1.7.0",
    "@types/gapi": "^0.0.36",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/seedrandom": "^2.4.28",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-typescript": "^2.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.1",
    "typescript-eslint-parser": "^22.0.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx,yml,css,sass,json,md}": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  }
}
